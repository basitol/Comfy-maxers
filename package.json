{
  "name": "comfy-maxers",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "16.13.1"
  },
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "yarn --cwd frontend start",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "YARN_CONFIG_PRODUCTION=false yarn install --prefix frontend && yarn run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basitol/Comfy-maxers.git"
  },
  "author": "Abdulbasit Quadri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/basitol/Comfy-maxers/issues"
  },
  "homepage": "https://github.com/basitol/Comfy-maxers#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "file-loader": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16"
  }
}
